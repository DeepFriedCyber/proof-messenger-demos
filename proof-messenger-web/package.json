{
  "name": "proof-messenger-web",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg --dev",
    "start": "python -m http.server 8000 --directory www",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "serve": "python server.py"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@types/react": "^18.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "c8": "^8.0.0",
    "jsdom": "^23.0.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}